@model IEnumerable<Sistratur.Modelos.Modelos.Vehiculo>

@{
    ViewBag.Title = "Index";
}

<br />
<h2>Lista de Vehículos</h2>
<hr />
<div class="panel-body">
    <table class="table table-condensed table-striped">
        <tr class="info text-primary" style="font-size:12.5px; text-align:center">
            <th>
                @Html.DisplayNameFor(model => model.Color.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Combustible.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoVehiculo.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MarcaVehiculo.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Modelo.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoVehiculo.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Placa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnioModelo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnioFabricacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ejes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ruedas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NroAsientos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NroPasajeros)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaRegistro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Categoria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FormulaRodante)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Potencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Carroceria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Kilometraje)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Alto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ancho)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Longitud)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cilindros)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cilindrada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CargaUtil)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PesoBruto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PesoNeto)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Color.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Combustible.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EstadoVehiculo.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MarcaVehiculo.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Modelo.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TipoVehiculo.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Placa)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AnioModelo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AnioFabricacion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ejes)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ruedas)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NroAsientos)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NroPasajeros)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FechaRegistro)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Categoria)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FormulaRodante)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Potencia)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Carroceria)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Kilometraje)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Alto)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ancho)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Longitud)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cilindros)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cilindrada)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CargaUtil)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PesoBruto)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PesoNeto)
                </td>
            </tr>
        }

    </table>
</div>
